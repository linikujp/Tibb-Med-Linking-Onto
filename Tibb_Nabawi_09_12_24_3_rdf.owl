    <owl:Class rdf:about="http://www.semanticweb.org/muna/ontologies/2018/6/TibbNabawi-ontology-15#Tuberculosis">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/muna/ontologies/2018/6/TibbNabawi-ontology-15#medical_condition"/>
        <rdfs:subClassOf>
            <owl:Restriction>
