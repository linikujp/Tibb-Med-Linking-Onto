    <owl:Class rdf:about="http://www.semanticweb.org/muna/ontologies/2018/6/TibbNabawi-ontology-15#Vitamin_D_deficiency_prevention">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/muna/ontologies/2018/6/TibbNabawi-ontology-15#medical_prevention"/>
        <rdfs:subClassOf>
            <owl:Restriction>
